  <body>
    
    <header class="header-area header-style-1 header-height-2">
      <div
        class="header-top"
        style="
          background-color: #4e7f5e;
          color: #ffffff;
          padding: 10px 20px;
          font-size: 14px;
        "
      >
        <div
          style="
            display: flex;
            justify-content: space-between;
            align-items: center;
            max-width: 1200px;
            margin: 0 auto;
          "
        >
          <!-- Contact Info -->
          <div>
            <i class="fas fa-phone-alt"></i>
            <a
            class="gruppo-regular"
              href="tel:+919747333006"
              style="color: #ffffff; text-decoration: none; margin-left: 5px; font-size: 20px; font-weight: bold;"
            >
              +91 8848530816
            </a>
          </div>

          

          <!-- User Links -->
          <div>
            <% if (userId != "") { %>
            <a
            class="gruppo-regular"
              href="/logout"
              style="color: #ffffff; text-decoration: none; margin-right: 15px; font-size: 17px; font-weight: bold;"
            >
              <i class="fas fa-sign-out-alt"></i> Log Out
            </a>
            <% } else { %>
            <a
            class="gruppo-regular"
              href="/login"
              style="color: #ffffff; text-decoration: none; margin-right: 15px; font-size: 17px; font-weight: bold;"
            >
              <i class="fas fa-user"></i> Log In / Sign Up
            </a>
            <% } %>
